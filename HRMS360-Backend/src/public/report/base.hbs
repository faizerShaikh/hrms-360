    <style>
      body{ font-size: 14px !important; } table tr td, table tr th {
      page-break-inside: avoid; margin: 4px 0 4px 0; } @media print { @page{
      margin:43px 25px } } table.question-table, th { border: 1px solid white;
      border-collapse: collapse; } .dont-break { page-break-before: always; }
      .break-after{ page-break-after:auto; } .break-before{
      page-break-before:always; } .chip-0 { color: #FF0000; padding: 5px;
      border-radius: 5px; text-align: center; } .chip-1 { color: #FF0000;
      padding: 5px; border-radius: 5px; text-align: center; } .chip-2 { color:
      #FCAE1E padding: 5px; border-radius: 5px; text-align: center; } .chip-3 {
      color: #525252; padding: 5px; border-radius: 5px; text-align: center; }
      .chip-4 { color: #25BF9E; padding: 5px; border-radius: 5px; text-align:
      center; } .chip-5 { color: #0069FF; padding: 5px; border-radius: 5px;
      text-align: center; }
    </style>
  </head>
  <body>
    <div class="report-container">
      {{{body}}}
    </div>
  </body>
