  <script>
  const renderStrengthAndDevNeedsFromChart = (id,id2,avg) => {
    if(!avg){
      avg = 0
    }
    document.getElementById(id).style.left = `${(avg* 100) / 4}%`;
    document.getElementById(id2).style.left = `${(avg* 100) / 4}%`;

  };

</script>

  <div class="dont-break break-inside-avoid-page w-full min-h-[1044px] h-[1044px] mx-auto  p-5">
  <div class="relative h-full">
    {{> dual-gap/cornerFrames }}
    <div class="bg-primary-light text-primary font-urbanist font-bold p-2 text-base uppercase">
